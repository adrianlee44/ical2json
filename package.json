{
  "name": "ical2json",
  "version": "3.0.0",
  "description": "A simple node package to convert ical to JSON",
  "main": "index.js",
  "files": [
    "bin/icaljson",
    "build/cli.js",
    "build/ical2json.js"
  ],
  "scripts": {
    "prepublishOnly": "tsc",
    "lint": "eslint .",
    "test": "npm run clean; npm run lint; tsc; npm run coverage",
    "report": "nyc report --reporter=text-lcov | coveralls",
    "coverage": "nyc ava",
    "clean": "rm -rf coverage/ build/"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:adrianlee44/ical2json.git"
  },
  "bugs": {
    "url": "https://github.com/adrianlee44/ical2json/issues"
  },
  "keywords": [
    "ical",
    "json",
    "converter"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "author": "Adrian Lee",
  "license": "MIT",
  "bin": "bin/ical2json",
  "devDependencies": {
    "@types/node": "^13.13.2",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "ava": "^3.7.0",
    "coveralls": "^3.0.11",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "esm": "^3.2.25",
    "mock-fs": "^4.11.0",
    "nyc": "^15.0.1",
    "prettier": "^2.0.5",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "commander": "^5.0.0"
  },
  "ava": {
    "require": [
      "esm"
    ],
    "files": [
      "test/*.js"
    ]
  }
}
