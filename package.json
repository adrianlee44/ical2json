{
  "name": "ical2json",
  "version": "3.1.2",
  "description": "A simple node package to convert ical to JSON",
  "type": "module",
  "exports": "./build/ical2json.js",
  "types": "build/ical2json.d.ts",
  "files": [
    "bin/icaljson",
    "build/cli.js",
    "build/ical2json.js"
  ],
  "scripts": {
    "prepublishOnly": "tsc",
    "compile": "tsc",
    "lint": "eslint . --ext .js,.ts",
    "test": "npm run clean; npm run lint; tsc; npm run coverage; npm run report",
    "report": "nyc report --reporter=lcov",
    "coverage": "nyc ava",
    "clean": "rm -rf coverage/ build/"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:adrianlee44/ical2json.git"
  },
  "bugs": {
    "url": "https://github.com/adrianlee44/ical2json/issues"
  },
  "keywords": [
    "ical",
    "json",
    "converter"
  ],
  "engines": {
    "node": ">=14.16.0"
  },
  "author": "Adrian Lee",
  "license": "MIT",
  "bin": "bin/ical2json",
  "devDependencies": {
    "@types/node": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "ava": "^4.3.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "execa": "^5.1.1",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "tempy": "^1.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "commander": "^6.2.1"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
