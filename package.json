{
  "name": "ical2json",
  "version": "3.1.0",
  "description": "A simple node package to convert ical to JSON",
  "main": "build/ical2json.js",
  "files": [
    "bin/icaljson",
    "build/cli.js",
    "build/ical2json.js"
  ],
  "scripts": {
    "prepublishOnly": "tsc",
    "lint": "eslint . --ext .js,.ts",
    "test": "npm run clean; npm run lint; tsc; npm run coverage; npm run report",
    "report": "nyc report --reporter=lcov",
    "coverage": "nyc ava",
    "clean": "rm -rf coverage/ build/"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:adrianlee44/ical2json.git"
  },
  "bugs": {
    "url": "https://github.com/adrianlee44/ical2json/issues"
  },
  "keywords": [
    "ical",
    "json",
    "converter"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "author": "Adrian Lee",
  "license": "MIT",
  "bin": "bin/ical2json",
  "devDependencies": {
    "@types/mock-fs": "^4.13.0",
    "@types/node": "^14.14.20",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "ava": "^3.15.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "esm": "^3.2.25",
    "mock-fs": "^4.13.0",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "commander": "^6.2.1"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
